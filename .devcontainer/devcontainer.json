{
	"name": "Python 3",
	"image": "mcr.microsoft.com/devcontainers/python:1-3.12-bullseye",
	"features": {
		"ghcr.io/devcontainers-extra/features/apt-get-packages:1": {},
		"ghcr.io/devcontainers-extra/features/black:2": {},
		"ghcr.io/devcontainers-extra/features/curl-apt-get:1": {},
		"ghcr.io/devcontainers-extra/features/node-exporter:1": {},
		"ghcr.io/itsmechlark/features/doppler:2": {},
		"ghcr.io/devcontainers-extra/features/go-task:1"	: {},
		"ghcr.io/devcontainers-extra/features/direnv:1"	: {},
		"ghcr.io/dhoeric/features/act:1"	: {}
	},
		"customizations": {
		"vscode": {
			"extensions": [
				"ms-vscode.makefile-tools",
				"ms-azuretools.vscode-docker",
				"GitHub.vscode-pull-request-github",
				"ms-python.mypy-type-checker",
				"doppler.doppler-vscode",
				"DhavalVira.django-import-libraries",
				"bibhasdn.django-html",
				"jitdev.copy-django-model-fields",
				"yardensachs.copy-python-path",
				"junstyle.vscode-django-support",
				"mkhl.direnv",
				"github.vscode-github-actions",
				"MohammedAbidNafi.gitwit",
				"stpn.vscode-graphql",
				"anteprimorac.html-end-tag-labels",
				"franneck94.vscode-python-config",
				"sourcery.sourcery",
				"task.vscode-task",
				"redhat.vscode-yaml",
				"GraphQL.vscode-graphql"
			]
		}
	}, "postCreateCommand": "curl --create-dirs -o $HOME/.postgresql/root.crt 'https://yo-mama.b-cdn.net/AIVIN_SSL_CERT.crt'"

}
